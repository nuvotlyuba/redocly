        "name": "Shop"
      }
    ],
    "components": {
      "schemas": {
        "BadRequestError": {
          "description": "Bad request error",
          "properties": {
            "name": {
              "enum": [
                "BadRequestError"
              ],
              "type": "string"
            },
            "message": {
              "example": "Ошибка запроса",
              "type": "string"
            }
          },
          "required": [
            "name",
            "message"
          ],
          "title": "BadRequestError",
          "type": "object",
          "x-http-status": 400
        },
        "ForbiddenError": {
          "description": "Forbidden error",
          "properties": {
            "name": {
              "enum": [
                "ForbiddenError"
              ],
              "type": "string"
            },
            "message": {
              "example": "Доступ запрещён",
              "type": "string"
            }
          },
          "required": [
            "name",
            "message"
          ],
          "title": "ForbiddenError",
          "type": "object",
          "x-http-status": 403
        },
        "InternalServerError": {
          "description": "Internal server error",
          "properties": {
            "name": {
              "enum": [
                "InternalServerError"
              ],
              "type": "string"
            },
            "message": {
              "example": "Ошибка сервера",
              "type": "string"
            }
          },
          "required": [
            "name",
            "message"
          ],
          "title": "InternalServerError",
          "type": "object",
          "x-http-status": 500
        },
        "NotFoundError": {
          "description": "Not found error",
          "properties": {
            "name": {
              "enum": [
                "NotFoundError"
              ],
              "type": "string"
            },
            "message": {
              "example": "Сущность не найдена",
              "type": "string"
            }
          },
          "required": [
            "name",
            "message"
          ],
          "title": "NotFoundError",
          "type": "object",
          "x-http-status": 404
        },
        "ServiceUnavailableError": {
          "description": "Service unavailable error",
          "properties": {
            "name": {
              "enum": [
                "ServiceUnavailableError"
              ],
              "type": "string"
            },
            "message": {
              "example": "Сервис временно недоступен",
              "type": "string"
            }
          },
          "required": [
            "name",
            "message"
          ],
          "title": "ServiceUnavailableError",
          "type": "object",
          "x-http-status": 503
        },
        "UnauthorizedError": {
          "description": "Unauthorized error",
          "properties": {
            "name": {
              "enum": [
                "UnauthorizedError"
              ],
              "type": "string"
            },
            "message": {
              "example": "Требуется авторизация",
              "type": "string"
            }
          },
          "required": [
            "name",
            "message"
          ],
          "title": "UnauthorizedError",
          "type": "object",
          "x-http-status": 401
        },
        "Nothing": {
          "description": "Пустой объект",
          "properties": {},
          "title": "Nothing",
          "type": "object"
        },
        "CouponCreateRequest": {
          "type": "object",
          "title": "CouponCreateRequest",
          "properties": {
            "discountQRId": {
              "type": "string",
              "title": "DiscountQRId",
              "description": "Id QR скидки",
              "example": "Pfb7w9_AKCLt90vTMNPcQ",
              "maxLength": 21,
              "minLength": 21
            }
          },
          "required": [
            "discountQRId"
          ]
        },
        "CouponCreateSuccess": {
          "type": "object",
          "title": "CouponCreateSuccess",
          "required": [
            "data"
          ],
          "properties": {
            "data": {
              "type": "object",
              "required": [
                "couponId",
                "discount",
                "shop",
                "QRUrl"
              ],
              "properties": {
                "couponId": {
                  "type": "string",
                  "title": "CouponId",
                  "description": "Id купона"
                },
                "QRUrl": {
                  "type": "string",
                  "title": "QrCode",
                  "description": "QR код в котором размещен URL купона"
                },
                "discount": {
                  "type": "object",
                  "required": [
                    "benefit",
                    "condition",
                    "infoHeader"
                  ],
                  "properties": {
                    "benefit": {
                      "type": "object",
                      "required": [
                        "amount",
                        "unit"
                      ],
                      "properties": {
                        "amount": {
                          "type": "integer",
                          "title": "DiscountAmount",
                          "minimum": 0,
                          "description": "Величина получаемая от акции"
                        },
                        "unit": {
                          "type": "string",
                          "title": "DiscountAmountUnit",
                          "description": "Единица измерения для amount (рубли, бонусы, ...)",
                          "maxLength": 100
                        }
                      }
                    },
                    "activityTimeEnd": {
                      "title": "ActivityTimeEnd",
                      "type": "string",
                      "description": "Срок годности купона"
                    },
                    "infoHeader": {
                      "type": "string",
                      "title": "DiscountInfoHeader",
                      "description": "Короткое описание к акции",
                      "maxLength": 200
                    },
                    "condition": {
                      "type": "string",
                      "title": "DiscountCondition",
                      "description": "Расширенное описание условия акции",
                      "maxLength": 500
                    }
                  },
                  "title": "CouponDiscount",
                  "description": "Информация о скидке для купона"
                },
                "shop": {
                  "type": "object",
                  "title": "CouponShops",
                  "required": [
                    "name",
                    "logo",
                    "floors"
                  ],
                  "properties": {
                    "name": {
                      "type": "string",
                      "description": "Имя партнера"
                    },
                    "logo": {
                      "type": "string",
                      "description": "url логотипа партнера"
                    },
                    "floors": {
                      "type": "array",
                      "items": {
                        "description": "Этажи расположения магазина"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "CouponItemSuccess": {
          "type": "object",
          "title": "CouponItemSuccess",
          "required": [
            "data"
          ],
          "properties": {
            "data": {
              "type": "object",
              "required": [
                "couponId",
                "status",
                "discount",
                "shop",
                "QRUrl",
                "mall"
              ],
              "properties": {
                "couponId": {
                  "type": "string",
                  "title": "CouponId",
                  "description": "Id купона"
                },
                "status": {
                  "type": "string",
                  "title": "Status",
                  "description": "Статус купона"
                },
                "QRUrl": {
                  "type": "string",
                  "title": "QrCode",
                  "description": "QR код в котором размещен URL купона"
                },
                "discount": {
                  "type": "object",
                  "required": [
                    "benefit",
                    "condition",
                    "infoHeader"
                  ],
                  "properties": {
                    "benefit": {
                      "type": "object",
                      "required": [
                        "amount",
                        "unit"
                      ],
                      "properties": {
                        "amount": {
                          "type": "integer",
                          "title": "DiscountAmount",
                          "minimum": 0,
                          "description": "Величина получаемая от акции"
                        },
                        "unit": {
                          "type": "string",
                          "title": "DiscountAmountUnit",
                          "description": "Единица измерения для amount (рубли, бонусы, ...)",
                          "maxLength": 100
                        }
                      }
                    },
                    "activityTimeEnd": {
                      "title": "ActivityTimeEnd",
                      "type": "string",
                      "description": "Срок годности купона"
                    },
                    "infoHeader": {
                      "type": "string",
                      "title": "DiscountInfoHeader",
                      "description": "Короткое описание к акции",
                      "maxLength": 200
                    },
                    "condition": {
                      "type": "string",
                      "title": "DiscountCondition",
                      "description": "Расширенное описание условия акции",
                      "maxLength": 500
                    }
                  },
                  "title": "CouponDiscount",
                  "description": "Информация о скидке для купона"
                },
                "shop": {
                  "type": "object",
                  "title": "CouponShops",
                  "required": [
                    "name",
                    "logo",
                    "floors"
                  ],
                  "properties": {
                    "name": {
                      "type": "string",
                      "description": "Имя партнера"
                    },
                    "logo": {
                      "type": "string",
                      "description": "url логотипа партнера"
                    },
                    "floors": {
                      "type": "array",
                      "items": {
                        "description": "Этажи расположения магазина"
                      }
                    }
                  }
                },
                "mall": {
                  "type": "object",
                  "title": "CouponMall",
                  "description": "Информация о торговом центре",
                  "required": [
                    "name",
                    "address"
                  ],
                  "properties": {
